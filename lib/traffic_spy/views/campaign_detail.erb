<div class="page-header">
  <h3>Campaign Events Analytics Summary: <%= params[:identifier] %> </h3>
</div>


<ul class="nav nav-tabs">
  <li> <a href="/sources/<%= params[:identifier] %>">Analytics Summary</a></li>
  <li class="active"><a href="/sources/<%= params[:identifier] %>/campaigns">Campaign Analytics</a></li>
</ul>

<h4>Event Statistics for: <%=params[:event_name]%></h4>
<p><a href="/sources/<%= params[:identifier] %>/events">View Events Listing</a></p>

<div class="row">
  <div class="span6">

<h5>Campaign Events Breakdown</h5>
    <table class="table table-hover table-bordered table-striped">
      <th>Event</th>
      <th>Count</th>

      <% @campaign_results.each do |result| %>
      <tr>
        <td><a href="/sources/<%= params[:identifier] %>/events/<%= result[0] %>"><%= result[0] %></a><br/></td>
        <td class="count_col"><%= result[1] %><br/></td>
      </tr>

      <% end %>

    </table>


  </div>
</div>
