<h2>URL Statistics</h2>

<h3>Identifier: <%=params[:identifier]%> </h3>
<h4>Url: /<%=params[:rel_path]%></h4>

<div class="row">
  <div class="span6">

    <h5>URL Response Times</h5>
    <table class="table table-bordered table-striped">
      <th>URL id</th>
      <th>Response Time</th>
      <% @url_results.each do |result| %>
      <tr>
        <td><%= result[0] %><br/></td>
        <td><%= result[1] %><br/></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>