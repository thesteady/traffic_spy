<div class="page-header">
  <h3>Event Analytics Summary: <%= params[:identifier] %> </h3>
</div>

<ul class="nav nav-tabs">
  <li> <a href="/sources/<%= params[:identifier] %>">Analytics Summary</a></li>
  <li class="active"><a href="#">Event Analytics</a></li>
  <li><a href="#">URL Analytics</a></li>
</ul>


<div class="row">
  <div class="span8">

    <h5>Events List</h5>
    <table class="table table-bordered table-striped">
      <th>Event Name</th>
      <th>Count</th>
      <% @event_results.each do |result| %>
      <tr>
        <td><a href="/sources/<%= params[:identifier] %>/events/<%= result[0] %>"><%= result[0] %></a><br/></td>
        <td class="count_col"><%= result[1] %><br/></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
