<h2>Event Analytics Summary</h2>


<h3>Identifier: <%= params[:identifier] %></h3>
<p><a href="/sources/<%= params[:identifier] %>">View Application Analytics</a></p>


<div class="row">
  <div class="span8">

    <h5>Events List</h5>
    <table class="table table-bordered table-striped">
      <th>Event Name</th>
      <th>Count</th>
      <% @event_results.each do |result| %>
      <tr>
        <td><a href="/sources/<%= params[:identifier] %>/events/<%= result[0] %>"><%= result[0] %></a><br/></td>
        <td class="count_col"><%= result[1] %><br/></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
